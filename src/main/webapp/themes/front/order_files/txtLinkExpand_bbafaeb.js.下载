define("waimai:widget/usercenter/order/txtLinkExpand",function(t,a,n){n.exports={init:function(){var t=this;t.$el=$("#user-order"),$.ajax({url:"/waimai/trade/wordchaincms?t="+(new Date).getTime(),type:"get",data:null,dataType:"json",success:function(a){if(a=a?a:[],t.$txtLinkExpand=t.$el.find('[data-node="summary-txtLinkExpand"]'),a.length>0){t.$txtLinkExpand.html("<span class='cms-icon'></span>"+a[0].text),t.$txtLinkExpand.attr("href",a[0].text_link);try{setTimeout(addNStat({da_src:"txtLinkExpandBk",da_act:"show"}),5)}catch(n){}}else t.$txtLinkExpand.attr("href","javascript:void(0);")}})}}});