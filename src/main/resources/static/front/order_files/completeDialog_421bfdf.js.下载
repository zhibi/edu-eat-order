define("waimai:widget/usercenter/order/completeDialog",function(require,exports,module){var ctTmpl=[function(_template_object){var _template_fun_array=[],fn=function(__data__){var _template_varName="";for(var name in __data__)_template_varName+="var "+name+'=__data__["'+name+'"];';eval(_template_varName),_template_fun_array.push('<div class="v-complete-order">    <div class="v-complete">        <div class="float-header">            <button class="close-btn" data-node="close-btn">╳</button>        </div>        <div class="v-complete-body">            您确认已经收到外卖？        </div>        <div class="v-complete-ops">            <button data-node="accept">                确认            </button>                        '),0==isCloseComment&&_template_fun_array.push('                <button data-node="comment">                    评价订单                </button>            '),_template_fun_array.push("        </div>    </div></div>"),_template_varName=null}(_template_object);return fn=null,_template_fun_array.join("")}][0],GlobalTips;module.exports={init:function(t,e,a,n,o){var i=this;return GlobalTips=t,e.reset(),i.$el=new e({html:ctTmpl({isCloseComment:o})}),i.$dialog=i.$el.getElement(),i.initEvent(),i.acceptCallback=a,i.commentCallback=n,i},initEvent:function(){this.$dialog.on("click",'[data-node="accept"]',$.proxy(this.accept,this)),this.$dialog.on("click",'[data-node="comment"]',$.proxy(this.comment,this)),this.$dialog.on("click",'[data-node="close-btn"]',$.proxy(this.hide,this))},show:function(){this.$el.show({fade:!0})},clickMask:function(t){this.hide(),t.stopPropagation(),t.preventDefault()},hide:function(){GlobalTips.hide(),this.$el.hide({fade:!0})},accept:function(){this.hide(),this.acceptCallback&&this.acceptCallback()},comment:function(){this.commentCallback&&this.commentCallback()}}});